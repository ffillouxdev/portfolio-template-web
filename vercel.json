{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "env": {
      "DATABASE_URL": "your-database-url",
      "NEXT_PUBLIC_API_URL": "your-api-url",
      "NEXTAUTH_URL": "your-next-auth-url"
    },
    "build": {
      "env": {
        "DATABASE_URL": "your-database-url"
      },
      "commands": {
        "prebuild": "npx prisma generate"
      }
    }
}
  